CREATE TABLE Course (
    courseId INTEGER PRIMARY KEY AUTOINCREMENT,
    courseName TEXT NOT NULL,
    requiredAttendance REAL NOT NULL
);

createCourse:
INSERT INTO Course (courseName, requiredAttendance)
VALUES (?, ?);

getAllCourses:
SELECT
    Course.courseId,
    Course.courseName,
    Course.requiredAttendance
FROM
    Course;
